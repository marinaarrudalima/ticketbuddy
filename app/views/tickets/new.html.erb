<%= form_with model: @ticket do |f| %>
  <%= f.label :artist  %>
  <%= f.text_field :artist %>
  <%= f.label :ticket_type  %>
  <%= f.text_field :ticket_type %>
  <%= f.label :price  %>
  <%= f.number_field :price %>
  <%= f.label :city  %>
  <%= f.text_field :city %>
  <%= f.label :event_date  %>
  <%= f.date_field   :event_date %>
  <%= f.submit %>
<% end %>
